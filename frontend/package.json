{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vite":"vite",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "tsc -b && vite build",
    "backend": "cd backend && uvicorn main:app --reload --host 127.0.0.1 --port 8000",
    "frontend": "cd frontend && vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "wait-on tcp:5173 tcp:5174 tcp:5175 && electron .",
    "start": "concurrently \"npm run frontend\" \"npm run electron\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.84.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "^34.1.1",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}
